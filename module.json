{
  "id": "ready-for-the-stage",
  "title": "Ready for the Stage: Theatre Inserts Extension",
  "description": "Changes to theatre inserts to make it more usable (IMO)",
  "version": "#{VERSION}#",
  "compatibility": {
    "minimum": 12,
    "verified": 13
  },
  "authors": [
    {
      "name": "ChasarooniZ"
    }
  ],
  "relationships": {
    "systems": [],
    "requires": [
      {
        "id": "socketlib",
        "type": "module",
        "manifest": "https://raw.githubusercontent.com/manuelVo/foundryvtt-socketlib/master/module.json"
      },
      {
        "id": "theatre",
        "type": "module"
      }
    ]
  },
  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "languages/en.json"
    }
  ],
  "esmodules": ["scripts/module.js"],
  "socket": true,
  "packs": [
    {
      "name": "ready-for-stage-macros",
      "label": "Ready for The Stage Macros",
      "path": "packs/ready-for-stage-macros",
      "type": "Macro",
      "ownership": {
        "PLAYER": "OBSERVER",
        "ASSISTANT": "OWNER"
      }
    },
    {
      "name": "ready-for-the-stage-help",
      "label": "Ready for The Stage",
      "path": "packs/ready-for-the-stage-help",
      "type": "JournalEntry",
      "ownership": {
        "PLAYER": "OBSERVER",
        "ASSISTANT": "OWNER"
      }
    }
  ],
  "url": "#{URL}#",
  "manifest": "#{MANIFEST}#",
  "download": "#{DOWNLOAD}#",
  "license": "LICENSE",
  "readme": "https://github.com/chasarooniZ/ready-for-the-stage/blob/master/README.md"
}
